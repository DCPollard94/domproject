@article{Perlman2004a,
abstract = {P. examines Eleutherna's trade during the Archaic period through the epigraphic and archaeological record. She uses comparative evidence from other parts of Crete to argue against a) the idea that Cretan commerce did not involve free citizens of cities (related to the traditional 'aristocratic' view of Cretans organized in syssitia, not engaged in commerce) and b) that the Cretan economy completely collapsed at the end of the Iron Age. She concludes that there is nothing to contradict the idea that Eleutherna's citizenry involved specialized craftsmen, and that its economy was not exclusively subsistence farming/husbandry.},
author = {Perlman, Paula},
doi = {10.1525/ca.2004.23.1.95},
